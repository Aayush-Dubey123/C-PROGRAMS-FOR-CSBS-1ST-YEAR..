// Online C compiler to run C program online
#include <stdio.h>
struct abc 
{
    int a;
    float b;
    char c;
}A1,A2;
int main() 
{
   A1.a=10;
   A1.b=22.5;
   A1.c='c';
   A2.a=20;
   A2.b=34.6;
   A2.c='n';
   printf("%d  %f  %c\n",A1.a,A1.b,A1.c);
    printf("%d  %f  %c",A2.a,A2.b,A2.c);
    return 0;
}
