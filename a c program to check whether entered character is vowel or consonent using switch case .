// Online C compiler to run C program online
#include <stdio.h>

int main()
{
  char ch ;
  printf("enter character = ");
  scanf("%c",&ch);
  switch(ch)
  {
    case 'a':
    case 'e':
    case 'i':
    case 'o':
    case 'u':
    case 'A':
    case 'E':
    case 'I':
    case 'O':
    case 'U':printf("its a VOWEL");break;
    default: printf(" consonent ");
  }
    return 0;
}
