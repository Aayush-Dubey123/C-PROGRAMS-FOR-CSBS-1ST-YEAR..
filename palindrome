#include <stdio.h> 
int main ()
{
    int num,ori,rev=0,d; 
   printf("enter number =");
   scanf("%d",&num);
   ori=num;
   while(num>0)
   {
       d=num%10;
       rev=rev*10+d;
       num=num/10;
   }
   if(rev==ori)
   {
       printf("number is palindrome");
   }
   else
   {
       printf("number is not palindrome");
   }
    return 0;
}
