// Online C compiler to run C program online
#include <stdio.h>

int main() 
{
   int x,y;
   printf("enter x and y =");
   scanf("%d%d",&x,&y);
   if(x>0 && y>0 )
    {
        printf("1st quadrant");
    }
     if(x<0 && y>0 )
    {
        printf("2nd quadrant");
    }
      if(x<0 && y<0 )
    {
        printf("3rd quadrant");
    }
      if(x>0 && y<0 )
    {
        printf("4th quadrant");
    }
      if(x==0 && y==0 )
    {
        printf("origin");
    }
      if(x==0 && y>0 )
    {
        printf("y axis ");
    }
      if(x>0 && y==0 )
    {
        printf("x axis");
    }
      if(x<0 && y==0 )
    {
        printf("-x axis");
    }
     if(x==0 && y<0 )
    {
        printf("-y axis");
    }
    return 0;
}
