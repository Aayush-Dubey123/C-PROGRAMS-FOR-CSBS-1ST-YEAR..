// Online C compiler to run C program online
#include <stdio.h>

int main()
{
  int month ;
  printf("enter month number = ");
  scanf("%d",&month);
  switch(month)
  {
    case 1:
    case 3:
    case 5:
    case 7:
    case 8:
    case 10:printf("its 30 days");break;
    case 2:printf("its a 28 days");break;
    case 4:
    case 6:
    case 9:
    case 11:
    case 12:printf("its a 31 days");break;
    default: printf("invalid input ");
  }
    return 0;
}
