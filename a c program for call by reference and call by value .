// Online C compiler to run C program online
#include <stdio.h>
void swap (int x,int y)
{
    int temp;
    temp = x;
    x=y;
    y=temp;
}
void main()
{
    int a=100,b=200;
    printf("before = %d and %d\n",a,b);
     swap(a,b)
     printf("after = %d and %d");
}
_____________________________________________________
// Online C compiler to run C program online
#include <stdio.h>
void swap (int *x,int *y)
{
    int temp;
    temp = *x;
    *x=*y;
    *y=temp;
}
void main()
{
    int a=100,b=200;
    printf("before = %d and %d\n",a,b);
     swap(&a,&b);
     printf("after = %d and %d",a,b);
}
