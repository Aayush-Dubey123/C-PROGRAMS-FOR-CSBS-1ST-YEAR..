// Online C compiler to run C program online
#include <stdio.h>
int main()
{
    int a[3][3],b[3][3],c[3][3],i,j;
    printf("enter 3x3 number FOR A=");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
        scanf("%d",&a[i][j]);
        }
    }
    printf("enterd matrix A =\n");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
        printf("%d ",a[i][j]);
        }
        printf("\n");
    }
     printf("enter 3x3 number for B=");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
        scanf("%d",&b[i][j]);
        }
    }
    printf("enterd matrix B =\n");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
        printf("%d ",b[i][j]);
        }
        printf("\n");
    }
     for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
           c[i][j]=a[i][j]+b[i][j];
        }  
    }
     printf("addition matrix c =\n");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
        printf("%d ",c[i][j]);
        }
        printf("\n");
    }
    
    return 0;
}
