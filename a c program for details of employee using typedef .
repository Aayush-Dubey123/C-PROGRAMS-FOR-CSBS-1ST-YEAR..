// Online C compiler to run C program online
#include <stdio.h>
typedef struct employee
{
   char name[10];
   int age;
   float sal;
}emp;
int main() 
{
    emp s[5];
    int i;
    for(i=0;i<5;i++)
    {
         printf("\n enter name,age and salary term for employee %d ="i+1);
          scanf("%s%d%f",&s[i].name,&s[i].age,&s[i].sal);
    }
   for(i=0;i<5;i++)
   {
 printf(" details of employee %d are =\n",i+1);
 printf("NAME =%s\n",s[i].name);
 printf("age =%d\n",s[i].age);
 printf("salary =%f\n",s[i].sal);
 printf("------------------\n");
   }
    return 0;
}
