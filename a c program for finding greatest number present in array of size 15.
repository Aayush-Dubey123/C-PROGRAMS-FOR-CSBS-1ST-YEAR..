// Online C compiler to run C program online
#include <stdio.h>
int main()
{
    int a[15],i,max;
    printf("enter 15 elements for array =");
    for(i=0;i<15;i++)
{
    scanf("%d",&a[i]);
}
max=a[0];
for(i=0;i<15;i++)
{
if(a[i]>max)
{
    max=a[i];
}
}
printf("%d is greatest among the elements in array",max);
    return 0;
}
