// Online C compiler to run C program online
#include <stdio.h>

int large (int *p,int size)
{
   int max,i;
   max=p[0];
   for(i=0;i<size;i++)
   {
       if(p[i]>max)
       {
           max=p[i];
       }
   }
   return (max);
}
void main()
{
    int a[5]={1,2,3,4,5};
    printf("largest =%d",large(a,5));
}
