// Online C compiler to run C program online
#include <stdio.h>
int main()
{
float m,cm,km,inch,feet;
printf("enter meter units =");
scanf("%f",&m);
cm = 100*m;
km = m/1000;
inch = cm/2.54;
feet = m/12;
printf("\nvalues in cm = %.2f\n values in km = %.2f\n values in inch = %.2f\n values in feet =%.2f ",cm,km,inch,feet);
return 0;
}
