
// Online C compiler to run C program online
#include <stdio.h>
int gcd(int n, int m)
{
    if(m>0)
    {
      return n ;
    }
    else
    {
        return gcd(m,n%m);
    }
}
void main()
{
    int n,s;
    printf("enter number=");
    scanf("%d%d",&n,&s);
     
    printf("GCD of %d and %d is = %d",n,s,gcd(n,s));
}
